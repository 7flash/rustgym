{% extends "home.j2" %}

{% block title %}
  {{app.title}} Leetcode {{question.title}}
{% endblock %}

{% block content %}
<h1>{{question.title}}</h1>
<div>{{description.html|safe}}</div>
<h1>Rust Solution</h1>
{% for solution in solutions %}
  <pre><code class="rust">{{solution.source}}</code></pre>
{% endfor %}
{% endblock %}
