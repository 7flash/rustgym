{% extends "home.j2" %}

{% block title %}
  {{app.title}} Leetcode Rust Solutions
{% endblock %}


{% block content %}
<table class="table-auto">
  <thead>
    <tr>
      <th>id</th>
      <th>Leetcode {{rows.len()}} Rust Solutions</th>
      <th>Level</th>
    </tr>
  </thead>
  <tbody>
    {% for row in rows %}
      <tr>
        <td>
          {{row.id}}
        </td>
        <td>
          <a href="{{row.href()}}"> {{row.title}}</a>
        </td>
        <td>
          {{ row.level_str() }}
        </td>
      </tr>
    {% endfor %}
  </tbody>
</table>
{% endblock %}
